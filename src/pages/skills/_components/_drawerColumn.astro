---
import Drawer from './_drawer.astro';

interface Props {
  skillCollection: {
    category: string;
    values: string[];
  }[];
}

const { skillCollection } = Astro.props;
---

<div class='flex flex-col gap-6'>
  {
    skillCollection.map((val) => {
      // @ts-ignore
      return <Drawer categoryTitle={val.category} values={val.values} />;
    })
  }
</div>
